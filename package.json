{
  "name": "react-ssr",
  "version": "0.0.1",
  "private": true,
  "description": "try to learn using React with server slide render deeply",
  "repository": "git@github.com:JounQin/react-ssr.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "engines": {
    "node": ">=7.10.0"
  },
  "scripts": {
    "dev": "cross-env DEBUG=hi:* node build/bin/start",
    "prod": "cross-env NODE_ENV=production yarn dev",
    "start": "yarn build && yarn prod",
    "build": "rimraf dist && yarn compile",
    "compile:base": "yarn compile:client && yarn compile:server",
    "compile:dev": "cross-env DEBUG=hi:* yarn compile:base",
    "compile": "cross-env NODE_ENV=production yarn compile:dev",
    "compile:client": "webpack --config build/webpack/client.babel.js --progress --hide-modules",
    "compile:server": "webpack --config build/webpack/server.babel.js --progress --hide-modules",
    "lint": "yarn lint:es",
    "lint:es": "eslint --max-warnings 10 .",
    "lint:fix": "yarn lint:es --fix",
    "pm2": "cross-env NODE_ENV=production PORT=4000 pm2 start build/bin/start.js --name=react-ssr"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "printWidth": 150
        }
      }
    ]
  },
  "dependencies": {
    "axios": "^0.17.0",
    "bootstrap-styl": "^5.0.8",
    "classnames": "^2.2.5",
    "koa": "^2.4.1",
    "koa-compress": "^2.0.0",
    "koa-logger": "^3.1.0",
    "koa-static": "^4.0.2",
    "nib": "^1.1.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-async-bootstrapper": "^1.1.2",
    "react-async-component": "^1.0.2",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "regenerator-runtime": "^0.11.1",
    "run-in-vm": "^1.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.1",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "eslint": "^4.14.0",
    "eslint-config-1stg": "^0.0.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "hash-sum": "^1.0.2",
    "html-webpack-plugin": "^2.30.1",
    "img-loader": "^2.0.0",
    "memory-fs": "^0.4.1",
    "null-loader": "^0.1.1",
    "pug": "^2.0.0-rc.4",
    "react-style-loader": "^2.0.1",
    "rimraf": "^2.6.2",
    "serialize-javascript": "^1.4.0",
    "ssr-webpack-plugin": "^1.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "stylus-px2rem": "^1.0.14",
    "sw-precache-webpack-plugin": "^0.11.4",
    "url-loader": "^0.6.1",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.1",
    "webpack-hot-middleware": "^2.21.0"
  }
}
