{
  "name": "react-ssr",
  "version": "0.0.1",
  "description": "try to learn using React with server slide render deeply",
  "repository": "git@github.com:JounQin/react-ssr.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env DEBUG=1stg:* babel-node server",
    "build:react:client": "webpack --config=build/react.client.babel.js --colors --progress",
    "build:react:server": "webpack --config=build/react.server.babel.js --colors --progress",
    "build:server": "webpack --config build/server.babel.js --colors --progress",
    "build:template": "mkdir -p dist/static && node build/template.js",
    "build": "rimraf dist && cross-env DEBUG=1stg:* NODE_ENV=production run-p build:react:client build:react:server build:server build:template",
    "start": "cross-env DEBUG=1stg:* NODE_ENV=production node dist/server",
    "sync:now": "bash build/sync-now.sh",
    "sync:pull": "bash build/sync-pull.sh",
    "sync:push": "bash build/sync-push.sh",
    "lint": "eslint . --max-warnings=10",
    "precommit": "lint-staged",
    "prune": "yarn install --production --ignore-optional --ignore-scripts --prefer-offline --force"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "babel": {
    "presets": [
      "@babel/env"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "globals": {
      "__DEV__": false,
      "__SERVER__": false
    },
    "extends": [
      "1stg/react"
    ]
  },
  "eslintIgnore": [
    "dist"
  ],
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --max-warnings=10 --fix",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "anujs": "^1.4.6",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.0",
    "debug": "^3.1.0",
    "koa": "^2.5.2",
    "koa-cash": "^2.2.0",
    "koa-compose": "^4.1.0",
    "koa-compress": "^3.0.0",
    "koa-convert": "^1.2.0",
    "koa-logger": "^3.2.0",
    "koa-static-cache": "^5.1.2",
    "lru-cache": "^4.1.3",
    "react": "^16.4.2",
    "react-async-bootstrapper": "^2.1.1",
    "react-async-component": "^2.0.0",
    "react-dom": "^16.4.2",
    "react-router": "^4.3.1",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "react-server-renderer": "^0.2.2",
    "serialize-javascript": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "apply-loader": "^2.0.0",
    "autoprefixer": "^9.1.3",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.15",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.0",
    "eslint": "^5.4.0",
    "eslint-config-1stg": "^3.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "glob": "^7.1.3",
    "html-minifier": "^3.5.20",
    "husky": "^1.0.0",
    "img-loader": "^3.0.0",
    "koa-webpack": "^5.1.0",
    "lint-staged": "^7.2.2",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.9.3",
    "normalize.css": "^8.0.0",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^3.0.0",
    "postcss-plugin-px2rem": "^0.7.0",
    "prettier": "^1.14.2",
    "prop-types": "^15.6.2",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "purgecss-webpack-plugin": "^1.2.1",
    "react-style-loader": "^2.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "ssr-webpack-plugin": "^2.0.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  }
}
